<div class="filter-container" style="display: none;">

    <div class="container-clear-x">
      <button class="clear">Clear all</button>

      <div class="x"> X </div>

    </div>

    <div class="filter-meters-container">
      <form>

        <div class="explain-filter-meters">
          What are you looking for?
        </div>

        <div class="stars">stars to add</div>
          <div class="filter-crowd"> 
              <p>Crowd</p>
              <input type="range" min="1" max="9" value="5" class="filter-slider">
      
             <div class="filter-labels-crowd"> 
                  <span>Pretty quiet</span> 
                  <span>Room for more</span> 
                  <span>Full house</span>
              </div>
          </div> 
      
          <div class="filter-hygiene"> 
            <p>Hygiene</p>
            <input type="range" min="1" max="9"  class="filter-slider">
            <div class="filter-labels-hygiene">
                <span>Clean</span> 
                <span>Decent</span> <span>Unimportant</span>
              </div>
           </div> 
      
          <div class="filter-atmosphere"> 
            <p>Atmosphere</p>
            <input type="range" min="1" max="9" value="5" class="filter-slider">
            <div class="filter-labels-atmo">
              <span>Dancing</span> <span>Chatting</span></div>
          </div> 
      
          <div class="filter-safety"> 
            <p>Safety</p>
            <input type="range" min="1" max="9" value="5" class="filter-slider">
           <div class="filter-labels-safety"> 
             <span>People feel safe</span> <span>Unimportant</span></div>
          </div> 

          
      
    
          <button class="apply-filter" >Apply filters</button>
      </form>



  </div>
</div>
<div class="filterby-container">
    <button>FILTER BY</button>
    <div class="set-display">
        <button>List</button>
        <button>Map</button>
    </div>
</div>  
</div>
</div>
